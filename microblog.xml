    body {
      margin-top: 0;
      font-family: "Google Sans", sans-serif;
      background: transparent;
      color: #333;
    }
    a { 
      text-decoration: none; 
      color: #455a64;
    } 
    .topbar {
      position: sticky;
      top: 0;
      background: #fff;
      border-bottom: 1px solid #eee;
      z-index: 10;
    }
    .topbar-inner {
      max-width: 1000px;
      margin: auto;
      display: flex;
      align-items: center;
      justify-content: space-between;
      padding: 10px 17px;
    }

    .profile {
      display: flex;
      align-items: center;
      gap: 10px;
    }
    .profile img {
      width: 35px;
      height: 35px;
      border-radius: 50%;
      margin-top: 4px;
    }
    .profile-name {
      font-size: 14px;
      font-weight: 600;
      line-height: 1.3;
      margin-left: -3px;
    }
    .description {
      font-size: 12px;
      color: #455a64;
      margin-left: -3px;
    }
    .menu {
      display: flex;
      gap: 14px;
      font-size: 13px;
      color: #455a64;
    }
   .follow {
      width: 60px;
      color: white;
      background: #3b4c55;
      border: 1px solid #3b4c55;
      border-radius: 10px;
      padding: 6px;
    }
    .container {
      max-width: 450px;
      margin: auto;
      padding: 0 0 60px;
    }
    .heading {
      font-size: 16px;
      color: #455a64;
      padding: 0 6px;
      margin-bottom:5px;
      text-align: left;
   }
    footer {
      text-align: center;
      font-size: 14px;
      color: #aaa;
      padding: 30px 0;
      margin-top: -30px;
      line-height: 1.6;
    }
    img {
      border-radius: 12px;
    }

#comments {
  font-size: 16px;
  line-height: 1.4;
  margin-top: 24px;
  border-left: 1px solid #eee;
  padding: 0 24px;
  background-color: #fff;
}
  /* Comment Author & Date */
.comments .comment-timestamp,
.comments .comment-date,
.comments .comment-date a,
.comments .datetime,
.comments .published,
.comments time {
  color: #455a64 !important;
  font-size: 13px;
}

/* Avatar */
.comment img {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}


/* Header */
.comment-header {
  font-size: 15px;
  line-height: 1.4;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
}

/* Reply - Delete */
.comment-actions a {
  font-size: 12px;
  color: #455a64;
  text-decoration: none;
  margin-right: 10px;
}

.comment-actions a:hover {
  color: #536471;
}

.post-card {
  border: 1px solid #eee;
  border-radius: 14px;
  padding: 15px 14px;
  margin: 16px 0; 
  background: #fff;  
}

.post-header {
  display: flex;
  gap: 12px;
  align-items: center;
}

.post-avatar {
  width: 38px;
  height: 38px;
  border-radius: 50%;
  margin-top: 2px;
}

.post-user {
  display: flex;
  flex-direction: column;
}

.post-name {
  font-size: 15px;
  font-weight: 600;
  line-height: 1.3;
  margin-left: -2px;
}

.post-date {
  font-size: 13px;
  color: #455a64;
  margin-left: -2px;
}

.post-content {
  margin-top: -21px;
  font-size: 16px;
  line-height: 1.4;
  white-space: pre-wrap;
  margin-left: 48px;
}

.post-footer {
  margin-top: -4px;
  padding-top: 10px;
  padding-bottom: 0;
  font-size: 14px;
  color: #455a64;
  margin-left: 49px;
}

.post-footer a {
  color: #455a64;
  font-weight: 500;
}
  ]]></b:skin>
</head>

